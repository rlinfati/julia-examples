   "metadata": {},
   "outputs": [],
   "source": [
    "for i in eachindex(x)\n",
    "    @show x[i]\n",
    "    if i == length(x) continue end\n",
    "    if x[i] == 0 x[i+1] = 0; continue end\n",
    "    if x[i] == 3 x[i+1] = 3; continue end\n",
    "    x[i+1] = rand() < p ? x[i] + 1 : x[i] - 1 \n",
    "    \n",
    "end"
   ]
